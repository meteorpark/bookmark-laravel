    "servers": [
        {
            "url": "https://api-bookmark.meteopark.dev/api/documentation",
            "description": "REST API Host"
        }
    ],
    "paths": {
        "/api/v1/users": {
            "post": {
                "tags": [
                    "user"
                ],
                "summary": "유저생성",
                "description": "유저를 만들어 봅시다..",
                "operationId": "store",
                "parameters": [
                    {
                        "name": "join_type",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "kakao",
                                "google",
                                "facebook"
                            ]
                        }
                    },
                    {
                        "name": "sns_id",
                        "in": "query",
                        "description": "SNS unique id",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Nickname or User's name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "profile_image",
                        "in": "path",
                        "description": "profile image url",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "successful operation"
                    },
                    "400": {
                        "description": "input error"
                    }
                }
            }
        }
    },
    "externalDocs": {
        "description": "Find out more about Swagger",
        "url": "http://swagger.io"
    }
}